#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/vite@5.0.6_@types+node@24.10.1_terser@5.44.1/node_modules/vite/bin/node_modules:/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/vite@5.0.6_@types+node@24.10.1_terser@5.44.1/node_modules/vite/node_modules:/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/vite@5.0.6_@types+node@24.10.1_terser@5.44.1/node_modules:/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/vite@5.0.6_@types+node@24.10.1_terser@5.44.1/node_modules/vite/bin/node_modules:/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/vite@5.0.6_@types+node@24.10.1_terser@5.44.1/node_modules/vite/node_modules:/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/vite@5.0.6_@types+node@24.10.1_terser@5.44.1/node_modules:/home/vimals/JmixProjects/multitenancy-test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../vite/bin/vite.js" "$@"
fi
